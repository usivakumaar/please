.dl > div:nth-child(even) {
    background: #fff;
}

.dl > div:nth-child(odd) {
    background: #efefef;
}


.dl > div.dl-h {
    font-size: 0.9rem;
    color: #949494;
    background: white;
}

    .dl > div.dl-h .dl-cl {
        padding-bottom: 8px;
        padding-top: 4px;
    }




    .dl-cl:not(.dl-cl-act) {
        padding-right: 40px;
    }

.dl-i .dl-cl:first-child {
    border-left: 1px solid #d1d1d1;
}

.dl-i .dl-cl:last-child {
    border-right: 1px solid #d1d1d1;
}

.dl-i > div:first-child .dl-cl:first-child {
    border-top: 1px solid #d1d1d1;
}


.dl-i:first-child.dl-cl,
.dl-i:nth-child(2) .dl-cl {
    border-top: 1px solid #d1d1d1;
}

    .dl-i:first-child .dl-cl:first-child,
    .dl-i:nth-child(2) .dl-cl:first-child {
        border-top-left-radius: 4px;
    }

    .dl-i:first-child .dl-cl:last-child,
    .dl-i:nth-child(2) .dl-cl:last-child {
        border-top-right-radius: 4px;
    }


.dl-i:last-child .dl-cl {
    border-bottom: 1px solid #d1d1d1;
}

    .dl-i:last-child .dl-cl:first-child {
        border-bottom-left-radius: 4px;
    }

    .dl-i:last-child .dl-cl:last-child {
        border-bottom-right-radius: 4px;
    }


.dl-cl-act {
    white-space: nowrap;
}



    .dl-cl-act > a,
    .dl-cl-act > button {
        display: inline-block;
        text-align: center;
    }

        .dl-cl-act > a:hover,
        .dl-cl-act > a:focus,
        .dl-cl-act > button:hover,
        .dl-cl-act > button:focus {
            text-decoration: none;
        }



#ResourceList {
    width: 100%;
    padding: 0 20px;
}




.clResourceName {
    min-width: 200px;
    width: 450px;
}

.clResourceRestrictions {
    width: 240px;
}

.clResourceValue {
    min-width: 240px;
}

.clResourceDate {
    min-width: 240px;
}

.clProfileActions > a:first-child:not(:last-child) {
    margin-right: 4px;
}


.action {
    background: white;
    padding: 5px 6px;
    transition: all 0.3s ease-out;
    border: 2px solid #48C6EA;
    border-radius: 4px;
    color: #48C6EA;
    min-width: 90px;
    min-height: 36px;
    box-shadow: inset -2px -2px 6px rgba(0,0,0, 0.08);
    text-decoration: none;
}

    .action.disabled,
    .action[disabled] {
        border: 2px solid #999;
        color: #999;
        box-shadow: inset -2px -2px 6px rgba(0,0,0, 0.08);
        cursor: default;
        pointer-events: none;
    }

    .action:not([disabled]):not(.disabled):hover,
    .action:not([disabled]):not(.disabled):active,
    .action:not([disabled]):not(.disabled):focus {
        border: 2px solid #ffd452;
        box-shadow: inset -2px -2px 6px rgba(0,0,0, 0.08), 0 0 5px rgba(47, 111, 167, 0.2);
        text-decoration: none;
    }


#modal {
    width: 100vw;
    height: 100vh;
    position: fixed;
    background: rgba(0,0,0, 0.3);
    text-align: center;
}

#modal-body {
    min-width: 40vw;
    max-width: 60vw;
    margin-top: 80px;
    display: inline-block;
    min-height: 100px;
    background: white;
    box-shadow: 2px 2px 8px rgba(0,0,0, 0.3);
}

#modal-close {
    width: 100%;
    text-align: right;
    padding-top: 16px;
    padding-right: 20px;
    font-family: GothamBook;
    font-weight: bold;
    font-size: 16px;
}

    #modal-close > a {
        cursor: pointer;
        display: inline-block;
        color: #333;
    }

        #modal-close > a:hover,
        #modal-close > a:focus,
        #modal-close > a:active {
            color: #333;
            text-decoration: none;
        }

#modal-message {
    width: 100%;
    text-align: left;
    padding: 16px 32px;
    font-family: GothamBook;
    font-weight: bold;
    margin-top: 8px;
    line-height: 1.5;
}

#modal-actions {
    width: 100%;
    text-align: right;
    padding-bottom: 16px;
    padding-right: 20px;
    font-family: GothamBook;
    font-weight: bold;
    margin-top: 16px;
}

    #modal-actions > a {
        cursor: pointer;
        display: inline-block;
        padding: 5px 20px;
        border: 2px solid #48C6EA;
        border-radius: 4px;
        transition: all 0.3s ease-out;
    }

        #modal-actions > a:not(:last-child) {
            margin-right: 8px;
        }

        #modal-actions > a:hover,
        #modal-actions > a:focus,
        #modal-actions > a:active {
            text-decoration: none;
            border: 2px solid #2f6fa7;
        }

body.hasModal {
    overflow: hidden;
}

.loadOverlay {
    background-repeat: no-repeat !important;
    background-position: center center !important;
    background-image: url('../Images/GIF/payprocessingLoading.gif') !important;
    background-color: rgba(255, 255, 255, 0.75) !important;
    background-size: auto 70px !important;
    position: absolute;
    opacity: 0;
    transition: opacity 0.3s;
}

    .loadOverlay.fadeIn {
        opacity: 1;
    }

.fld-form {
    display: table;
    table-layout: fixed;
    margin: 0 auto;
    margin-bottom: 30px;
}

.fld {
    display: table-row;
}

    .fld > div {
        padding: 4px 20px;
        display: table-cell;
    }

.fld-label {
    min-width: 300px;
}

.fld-value > *:not(.action),
.dl-cl > input,
.styleInput
{
    display: inline-block;
    padding: 10px;
    border-radius: 8px;
    border: 2px solid #48C6EA;
    outline: 0px;
    box-shadow: inset -2px -2px 6px rgba(0,0,0, 0.08);
    transition: all 0.15s;
}



    .dl-cl
    .fld-value > *[readonly],
    .fld-value > *.readonly,
    .styleInput[readonly],
    .styleInput.readonly {
        background: #ffffff;
        border: 2px solid #48C6EA;
        box-shadow: inset -2px -2px 6px rgba(0,0,0, 0.08) !important;
    }

.fld.hasError .fld-value > *:not(.action),
.hasError .styleInput,
.styleInput.hasError {
    border-color: red !important;
}

.dl-cl > input {
    margin: 0;
}

    .fld-value > *:not(.action):active,
    .fld-value > *:not(.action):focus,
    .fld-value > *:not(.action):hover,
    .dl-cl > input:active,
    .dl-cl > input:focus,
    .dl-cl > input:hover,
    .styleInput:active,
    .styleInput:focus,
    .styleInput:hover {
        border-color: #ffd452;
        box-shadow: inset -2px -2px 6px rgba(0,0,0, 0.08), 0 0 3px rgba(255, 212, 82, 1);
    }

    .fld-value > *[disabled],
    .fld-value > *.disabled,
    .dl-cl > input[disabled],
    .styleInput[disabled],
    .styleInput.disabled {
        border-color: #ccc !important;
    }

.form-actions {
    padding: 0 20px;
}


.fileSelector.fileName {
    cursor: pointer;
}

.fileSelector.fileName.notSelected {
    color: #999;
}

.fileSelector.fileInput {
    display: none;
}

.clResourceActions a {
    width: 90px;
}

#Error {
}

    #Error h2 {
        font-size: 2rem;
        font-family: GothamBook;
    }

    #Error p {
        font-size: 1.5rem;
        font-family: GothamBook;
    }

.form-status {
    display: inline-block;
    margin-left: 8px;
}

    .form-status .error {
        color: red;
    }

    .form-status .success {
    }

.fld-error {
    color: red;
    font-size: 0.85rem;
    position: relative;
    line-height: 1.4;
    display: table-row;
}

    .fld-error > div {
        display: table-cell;
    }

    .fld-error .fld-message ul:before {
        content: '*';
        display: inline;
        position: absolute;
        margin-left: -8px;
    }

    .fld-error .fld-message {
        padding: 0 32px;
    }

.form-actions input {
    margin: 0;
}

.fld-error + .fld > div {
    padding-top: 16px;
}


.dl-i.missing .clResourceName:after {

    content: '*';
    color: red;
    position: relative;
}

.dl.anyMissing:after {
    content: '* The resource has not been created, is missing a value or the associated file has been deleted.';
    color: red;
    position: absolute;
    margin-top: 8px;
    margin-left: 8px;
    font-size: 0.95rem;
}

.profileBreadcrumb,
.profileBreadcrumb:hover,
.profileBreadcrumb:active,
.profileBreadcrumb:focus {
    color: #1cb5e3;
    text-decoration: none;
}
